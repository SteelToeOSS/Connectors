{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication",
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  },
  "gemfire": {
    "client": {
      "properties": {
        "log-level": "fine",
        "connect-timeout": "100ms"
      }
    }
  },
  "AllowedHosts": "*"
}
